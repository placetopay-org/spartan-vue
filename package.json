{
  "name": "@placetopay/spartan-vue",
  "version": "2.0.0-beta.89",
  "type": "module",
  "description": "spartan vue lib",
  "main": "./dist/spartan-vue.umd.cjs",
  "module": "./dist/spartan-vue.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/spartan-vue.js",
      "require": "./dist/spartan-vue.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./locales": "./dist/locales/index.ts",
    "./plugin": "./dist/plugin.js",
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "url": "git+https://github.com/placetopay-org/spartan-vue.git"
  },
  "scripts": {
    "dev": "storybook dev -p 6006 --no-open",
    "build": "vite build && rollup -c && npm run build:types && cp -r src/locales dist/locales",
    "build:storybook": "storybook build",
    "build:rollup": "rollup -c",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "typecheck": "vue-tsc --noEmit",
    "format:check": "prettier . --check",
    "format": "prettier . --write",
    "test": "vitest watch --ui --coverage",
    "test:pipe": "vitest run --coverage",
    "lint": "npx eslint --ext .ts,.vue --fix ./",
    "lint:check": "npx eslint --ext .ts,.vue .",
    "foli": "npm run format & npm run lint",
    "foli:check": "npm run format:check & npm run lint:check"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "vue": "3.3.4",
    "vue-i18n": "^9.9.0"
  },
  "peerDependencies": {
    "vue": "3.3.4",
    "vue-i18n": "^9.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
