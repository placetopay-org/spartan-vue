{
  "name": "@placetopay/spartan-vue",
  "version": "2.0.0-beta.39",
  "type": "module",
  "description": "spartan vue lib",
  "main": "./dist/spartan-vue.umd.cjs",
  "module": "./dist/spartan-vue.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/*",
    "src/locales/*",
    "src/expose/tailwindcss.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/spartan-vue.js",
      "require": "./dist/spartan-vue.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./locales/*": "./src/locales/*",
    "./tailwindcss.js": "./src/expose/tailwindcss.js",
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "url": "git+https://github.com/placetopay-org/spartan-vue.git"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "vite build && npm run build:types",
    "build:storybook": "storybook build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "format:check": "prettier . --check",
    "format": "prettier . --write",
    "lint": "npx eslint --ext .ts,.vue --fix ./",
    "lint:check": "npx eslint --ext .ts,.vue .",
    "foli": "npm run format & npm run lint",
    "foli:check": "npm run format:check & npm run lint:check"
  },
  "dependencies": {
    "vue": "3.3.4",
    "vue-i18n": "^9.2.2"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@headlessui/vue": "1.7.15",
    "@heroicons/vue": "2.0.18",
    "@intlify/unplugin-vue-i18n": "^0.13.0",
    "@storybook/addon-a11y": "7.1.1",
    "@storybook/addon-designs": "7.0.1",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-interactions": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/blocks": "7.1.1",
    "@storybook/testing-library": "0.2.0",
    "@storybook/vue3": "7.1.1",
    "@storybook/vue3-vite": "7.1.1",
    "@tailwindcss/forms": "0.5.4",
    "@tailwindcss/typography": "0.5.9",
    "@tanstack/vue-table": "^8.10.7",
    "@types/lodash": "^4.14.198",
    "@types/node": "20.4.5",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.14",
    "chromatic": "6.20.0",
    "class-variance-authority": "^0.7.0",
    "dayjs": "1.11.9",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "litepie-datepicker-tw3": "0.0.5",
    "lodash": "^4.17.21",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.1.1",
    "storybook-addon-pseudo-states": "2.1.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "uuid": "^9.0.1",
    "vite": "4.4.8",
    "vue-currency-input": "^3.0.5",
    "vue-imask": "7.1.3",
    "vue-tsc": "1.8.8"
  },
  "bugs": {
    "url": "https://github.com/placetopay-org/spartan-vue/issues"
  },
  "readme": "ERROR: No README data found!",
  "homepage": "https://github.com/placetopay-org/spartan-vue#readme"
}
